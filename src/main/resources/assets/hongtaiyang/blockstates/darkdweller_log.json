{
  "multipart": [
    {
      "when" : {
        "axis": "x"
      },
      "apply" : {
        "model": "hongtaiyang:block/darkdweller_log",
        "x": 90,
        "y": 90
      }
    },
    {
      "when" : {
        "axis": "y"
      },
      "apply" : {
        "model": "hongtaiyang:block/darkdweller_log"
      }
    },
    {
      "when" : {
        "axis": "z"
      },
      "apply" : {
        "model": "hongtaiyang:block/darkdweller_log",
        "x": 90
      }
    },



    {
      "when" : {
        "OR": [
          { "north": 1 },
          { "num_log_branches": "0|1", "axis": "z" }
        ]
      },
      "apply" : {
        "model": "hongtaiyang:block/darkdweller_log_branch"
      }
    },
    {
      "when" : {
        "OR": [
          { "east": 1 },
          { "num_log_branches": "0|1", "axis": "x" }
        ]
      },
      "apply" : {
        "model": "hongtaiyang:block/darkdweller_log_branch",
        "y": 90
      }
    },
    {
      "when" : {
        "OR": [
          { "south": 1 },
          { "num_log_branches": "0|1", "axis": "z" }
        ]
      },
      "apply" : {
        "model": "hongtaiyang:block/darkdweller_log_branch",
        "y": 180
      }
    },
    {
      "when" : {
        "OR": [
          { "west": 1 },
          { "num_log_branches": "0|1", "axis": "x" }
        ]
      },
      "apply" : {
        "model": "hongtaiyang:block/darkdweller_log_branch",
        "y": 270
      }
    },
    {
      "when" : {
        "OR": [
          { "up": 1 },
          { "num_log_branches": "0|1", "axis": "y" }
        ]
      },
      "apply" : {
        "model": "hongtaiyang:block/darkdweller_log_branch",
        "x": 270
      }
    },
    {
      "when" : {
        "OR": [
          { "down": 1 },
          { "num_log_branches": "0|1", "axis": "y" }
        ]
      },
      "apply" : {
        "model": "hongtaiyang:block/darkdweller_log_branch",
        "x": 90
      }
    },
    
    
    
    {
      "when" : {
        "AND": [
          { "north": 2 },
          {
            "OR": [
              { "num_log_branches": "2|3|4|5|6" },
              { "num_log_branches": "0|1", "axis":  "x|y" }
            ]
          }
        ]
      },
      "apply" : {
        "model": "hongtaiyang:block/darkdweller_log_thin_branch"
      }
    },
    {
      "when" : {
        "AND": [
          { "east": 2 },
          {
            "OR": [
              { "num_log_branches": "2|3|4|5|6" },
              { "num_log_branches": "0|1", "axis":  "y|z" }
            ]
          }
        ]
      },
      "apply" : {
        "model": "hongtaiyang:block/darkdweller_log_thin_branch",
        "y": 90
      }
    },
    {
      "when" : {
        "AND": [
          { "south": 2 },
          {
            "OR": [
              { "num_log_branches": "2|3|4|5|6" },
              { "num_log_branches": "0|1", "axis":  "x|y" }
            ]
          }
        ]
      },
      "apply" : {
        "model": "hongtaiyang:block/darkdweller_log_thin_branch",
        "y": 180
      }
    },
    {
      "when" : {
        "AND": [
          { "west": 2 },
          {
            "OR": [
              { "num_log_branches": "2|3|4|5|6" },
              { "num_log_branches": "0|1", "axis":  "y|z" }
            ]
          }
        ]
      },
      "apply" : {
        "model": "hongtaiyang:block/darkdweller_log_thin_branch",
        "y": 270
      }
    },
    {
      "when" : {
        "AND": [
          { "up": 2 },
          {
            "OR": [
              { "num_log_branches": "2|3|4|5|6" },
              { "num_log_branches": "0|1", "axis":  "x|z" }
            ]
          }
        ]
      },
      "apply" : {
        "model": "hongtaiyang:block/darkdweller_log_thin_branch",
        "x": 270
      }
    },
    {
      "when" : {
        "AND": [
          { "down": 2 },
          {
            "OR": [
              { "num_log_branches": "2|3|4|5|6" },
              { "num_log_branches": "0|1", "axis":  "x|z" }
            ]
          }
        ]
      },
      "apply" : {
        "model": "hongtaiyang:block/darkdweller_log_thin_branch",
        "x": 90
      }
    }
  ]
}