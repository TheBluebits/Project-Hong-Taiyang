{
  "multipart": [
    {
      "when": {
        "axis": "x"
      },
      "apply": {
        "model": "hongtaiyang:block/darkdweller_log_horizontal"
      }
    },
    {
      "when": {
        "axis": "y"
      },
      "apply": {
        "model": "hongtaiyang:block/darkdweller_log"
      }
    },
    {
      "when": {
        "axis": "z" 
      },
      "apply": {
        "model": "hongtaiyang:block/darkdweller_log_horizontal",
        "y": 90
      }
    },



    {
      "when" : {
        "north": true
      },
      "apply" : {
        "model": "hongtaiyang:block/darkdweller_log_branch"
      }
    },
    {
      "when" : {
        "east": true
      },
      "apply" : {
        "model": "hongtaiyang:block/darkdweller_log_branch",
        "y": 90
      }
    },
    {
      "when" : {
        "south": true
      },
      "apply" : {
        "model": "hongtaiyang:block/darkdweller_log_branch",
        "y": 180
      }
    },
    {
      "when" : {
        "west": true
      },
      "apply" : {
        "model": "hongtaiyang:block/darkdweller_log_branch",
        "y": 270
      }
    },
    {
      "when" : {
        "above": true
      },
      "apply" : {
        "model": "hongtaiyang:block/darkdweller_log_branch",
        "x": 270
      }
    },
    {
      "when" : {
        "below": true
      },
      "apply" : {
        "model": "hongtaiyang:block/darkdweller_log_branch",
        "x": 90
      }
    }
  ]
}